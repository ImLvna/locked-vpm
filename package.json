    "@types/semver": "^7.5.7",
    "@types/unzipper": "^0.10.9",
    "husky": "^8.0.3",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "archiver": "^6.0.1",
    "express": "^4.18.2",
    "multer": "1.4.5-lts.1",
